import java.util.LinkedList;
public class classroom{
  public static void main(String ar[]){
    LinkedList<Integer> ll=new LinkedList<>();
    
    ll.addLast(1);
    ll.addLast(2);
    ll.addFirst(0);
    ll.addLast(3);
    System.out.println(ll);
    
    ll.removeLast();
    ll.removeFirst();
    System.out.println(ll);
  }
}
//Java Collection Framework
